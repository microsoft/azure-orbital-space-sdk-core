{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "System.Net.Http": "Error",
            "Microsoft.Azure.SpaceFx": "Debug",
            "Microsoft.Azure.SpaceFx.Core.Services.MessageReceiver": "Debug",
            "Microsoft.Azure.SpaceFx.Core.Services.HeartbeatService": "Debug",
            "Microsoft.Azure.SpaceFx.Core.Services.ResourceUtilizationMonitor": "Debug"
        },
        "Console": {
            "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] "
        }
    },
    "AllowedHosts": "*",
    "Kestrel": {
        "EndpointDefaults": {
            "Protocols": "Http2"
        }
    }
}